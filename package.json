{
	"name": "fathom-mcp-server",
	"version": "1.0.0",
	"description": "Fathom meeting transcription MCP server",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"fathom-mcp-server": "dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.ts",
		"start": "node dist/index.js",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"test": "node --test dist/*.test.js",
		"test:watch": "node --test --watch dist/*.test.js",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"mcp",
		"model-context-protocol",
		"fathom",
		"meeting",
		"transcription"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.20.2",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.1",
		"@types/node": "^24.9.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	}
}
